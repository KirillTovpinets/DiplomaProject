@{
    ViewBag.Title = "Изменения";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="content">
    <h1>Изменения</h1>
    @{
        int hasChan = 0;
        foreach (HtmlInputs.Models.Changes item in ViewBag.chan)
        {
            if (item.UserId.ToString().Equals(Session["LogedUserId"].ToString()))
            {
                hasChan = 1;
                <div>
                    <p>@item.Content</p>
                    <p>@item.Date.ToShortDateString()</p>
                </div>
            }
        }
        if (hasChan == 0)
        {
            <p>Изменений в ближайшее время не наблюдается</p>
        }
    }
</div>
<div class="miss-buttons">
    <a href="~/Profile/AddChanges"><input type="button" value="Добавить" /></a>
</div>